        {tokenAdmin && <Route exact path=\"/home\" element={<Home />} />}\n        <Route exact path=\"/add\" element={<AddUser />} />\n        {tokenAdmin && <Route exact path=\"/add\" element={<AddUser />} />}\n        <Route exact path=\"*\" element={<SignIn />} />\n      </Routes>\n    </Router>\n  )\n}\n\nexport default App\n","/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/AddUser.js",[],"/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/Home.js",[],"/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/pages/SignIn/SignIn.js",["50"],"import { React, useEffect, useState } from 'react'\nimport image from '../../images/office-2.jpeg'\nimport { useNavigate } from 'react-router-dom'\nimport axios from 'axios'\nimport logo from '../../images/logo.svg'\n\nfunction SignIn() {\n  const [pass, setPass] = useState('password')\n  const [eye, setEye] = useState('fa fa-eye-slash')\n\n  const [name, setName] = useState('')\n  const [password, setPassword] = useState('')\n  const navigate = useNavigate()\n\n  const [namePassErr, setNamePassErr] = useState(false)\n  // const [passErr, setPassErr] = useState(false)\n  const [err, setErr] = useState(false)\n\n  const onClickEye = () => {\n    if (pass === 'password') {\n      setPass('text')\n      setEye('fa fa-eye')\n    } else {\n      setPass('password')\n      setEye('fa fa-eye-slash')\n    }\n  }\n\n  const tokenAdmin = localStorage.getItem('tokenAdmin')\n  const tokenUser = localStorage.getItem('tokenUser')\n\n  useEffect(() => {\n    if (tokenAdmin) {\n      navigate('/home')\n    } else if (tokenUser) {\n      navigate('/dashboard')\n    } else {\n      navigate('/')\n    }\n  }, [tokenAdmin, tokenUser, navigate])\n\n  const onSubmitForm = async (e) => {\n    e.preventDefault()\n    try {\n      const result = await axios.post(\n        `${process.env.REACT_APP_API_URL}api/login`,\n        { name, password }\n      )\n\n      if (result.data.status === 200) {\n        if (result.data.response.user.admin === true) {\n          localStorage.setItem(\n            'tokenAdmin',\n            JSON.stringify(result.data.response.token)\n          )\n          navigate('/home')\n        } else if (result.data.response.user.admin === false) {\n          localStorage.setItem(\n            'tokenUsers',\n            JSON.stringify(result.data.response.token)\n          )\n          navigate('/dashboard')\n        }\n      }\n    } catch (error) {\n      setName(name)\n      setPassword('')\n      if (error.response.status === 422) {\n        setErr(true)\n      }\n      if (error.response.status === 401) {\n        setNamePassErr(true)\n      }\n    }\n  }\n\n  useEffect(() => {\n    if (namePassErr === true || err === true) {\n      const timeId = setTimeout(() => {\n        // After 5 seconds set the show value to false\n        if (namePassErr) {\n          setNamePassErr(false)\n        }\n        if (err) {\n          setErr(false)\n        }\n      }, 5000)\n\n      return () => {\n        clearTimeout(timeId)\n      }\n    }\n  }, [err, namePassErr])\n\n  return (\n    <div className=\"main\">\n      <div className=\"row\">\n        <div className=\"left-side col-md-6 col-sm-12\">\n          <img src={logo} className=\"login-logo\" />\n          <img src={image} alt=\"background\" className=\"back-image\" />\n        </div>\n        <div className=\"right-side col-md-6 col-sm-12 d-flex flex-column justify-content-center px-5\">\n          <h1>\n            Welcome to\n            <span className=\"d-block\"> Office Administration System</span>\n          </h1>\n          <p>We make job life easy for everone</p>\n          <div\n            className={`alert alert-danger text-center m-3 ${\n              err === true ? 'd-block' : 'd-none'\n            }`}\n            role=\"alert\">\n            Name or password cannot be empty!\n          </div>\n          <div\n            className={`alert alert-danger text-center m-3 ${\n              namePassErr === true ? 'd-block' : 'd-none'\n            }`}\n            role=\"alert\">\n            Name or Password is incorrect!\n          </div>\n          <form className=\"my-3\" onSubmit={onSubmitForm}>\n            <div className=\"form-group mb-3\">\n              <label>User Name</label>\n              <input\n                onChange={(e) => {\n                  setName(e.target.value)\n                }}\n                type=\"text\"\n                className=\"form-control\"\n                placeholder=\"Username\"\n                aria-label=\"Username\"\n                aria-describedby=\"basic-addon1\"\n              />\n            </div>\n            <div className=\"form-group mb-3\">\n              <label htmlFor=\"exampleInputPassword1\">Password</label>\n              <input\n                onChange={(e) => {\n                  setPassword(e.target.value)\n                }}\n                type={pass}\n                className=\"form-control\"\n                id=\"exampleInputPassword1\"\n                placeholder=\"&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;\"\n              />\n\n              <span className=\"togglePassword\" onClick={onClickEye}>\n                <i className={eye}></i>\n              </span>\n            </div>\n            <div className=\"form-group\">\n              <button className=\"btn btn-success float-right\" type=\"submit\">\n                Login\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default SignIn\n","/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/pages/Dashboard/Dashboard.js",["51","52","53","54","55","56","57","58","59"],"/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/pages/Categories/Categories.js",["60","61","62"],"/home/narminali/Desktop/vabiss/narmin-oms-front/front/src/components/Sidenav.js",["63","64","65","66","67","68","69","70","71"],"import React, { useState } from 'react'\nimport { Link } from 'react